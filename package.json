{
  "name": "libram",
  "version": "0.4.0",
  "description": "JavaScript helper library for KoLmafia",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/Loathing-Associates-Scripting-Society/libram",
  "author": "Samuel Gaus <sam@gaus.co.uk>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn run tsc && yarn run babel --extensions .ts src -d dist",
    "clean": "rm -rf dist",
    "docs": "yarn run typedoc",
    "format": "yarn run prettier --write .",
    "lint": "yarn run eslint src --ext .ts && yarn run prettier --check .",
    "prepublishOnly": "yarn run build",
    "updateProps": "yarn run ts-node ./tools/parseDefaultProperties.ts"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/compat-data": "^7.15.0",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.1",
    "@types/lodash-es": "^4.17.4",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-libram": "^0.1.9",
    "husky": "^4.3.6",
    "java-parser": "^1.4.0",
    "jest": "^27.1.0",
    "lint-staged": ">=10",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.19",
    "typescript": "^4.1.3",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.9",
    "core-js": "3.15.2",
    "kolmafia": "^1.3.0",
    "lodash": "^4.17.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": "prettier --write"
  },
  "sideEffects": false
}
